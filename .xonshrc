aliases['e'] = 'emacs'
aliases['g'] = 'git'
$PATH.append('%s/bin' % $HOME)
$PATH.append('%s/.local/bin' % $HOME)

import os.path
if not os.path.isfile('%s/private/.ssh/git' % $HOME):
    encfs ~/.encfs ~/private/

/usr/bin/keychain $HOME/.ssh/git

xontrib load vox

$PROJECT_DIRS = ["~/projects"]
xontrib load avox

# pyenv
$PYENV_ROOT = '%s/.pyenv' % $HOME
$PATH.insert(0, '%s/bin' % $PYENV_ROOT)
