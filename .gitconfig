[color]
  ui = auto
[alias]
  s  = status
  c  = commit
  a  = add
  p  = push
  st = status
  co = checkout
  cm = commit
  br = branch
  df = diff
  dw = diff --word-diff
  dc = diff --word-diff --word-diff-regex=.
  pom = push origin master
[push]
	default = simple

[diff "utf16diff"]
	textconv = "iconv -f utf-16 -t utf-8"
